    <CheatEntry>
      <ID>8731</ID>
      <Description>"Force Prefixes"</Description>
      <LastState/>
      <VariableType>Auto Assembler Script</VariableType>
      <AssemblerScript>// Force Prefix Addr
// old: define(force_prefix, Terraria.Item::Prefix+C0)
define(force_prefix, Terraria.Item::Prefix+BD)
// Invalid Prefixes Addr
define(invalid_prefix, Terraria.Item::Prefix+227C)
define(invalid_prefix_ret, Terraria.Item::Prefix+2282)
// Melee Weaps Addr
define(legendary, Terraria.Item::Prefix+933)
define(legendary_ret, Terraria.Item::Prefix+93E)
// Melee Fix
define(godly, Terraria.Item::Prefix+BE6)
define(godly_ret, Terraria.Item::Prefix+BEB)
// Ranged Weaps Addr
define(unreal, Terraria.Item::Prefix+10EA)
define(unreal_ret, Terraria.Item::Prefix+10F5)
// Magical Weaps Addr
define(mythical, Terraria.Item::Prefix+1656)
define(mythical_ret, Terraria.Item::Prefix+1661)
// Magical Fix (eg. Magical Harp / Last Prism)
define(demonic_ret, Terraria.Item::Prefix+179E)
// Accessories Addr
define(accessories, Terraria.Item::Prefix+1934)

[ENABLE]
assert(force_prefix, 75 02)
assert(invalid_prefix, 89 55 DC DB 45 DC)
assert(legendary, 0F 85 33 10 00 00 BB 51 00 00 00)
assert(godly, 83 F8 0B 75 05)
assert(unreal, 0F 85 7C 08 00 00 BB 52 00 00 00)
assert(mythical, 0F 85 10 03 00 00 BB 53 00 00 00)
assert(accessories, 6A 51 8B 4D E0 BA 3E)
alloc(mem, 2048)
label(flag)
label(invalid_prefix_mem)
label(legendary_mem)
label(godly_mem)
label(unreal_mem)
label(mythical_mem)

force_prefix:
db EB

invalid_prefix:
jmp invalid_prefix_mem
nop

legendary:
jmp legendary_mem
nop

godly:
jmp godly_mem

unreal:
jmp unreal_mem
nop

mythical:
jmp mythical_mem
nop

// Accessories
accessories:
db 6A 48 8B 4D E0 BA 48
// ?? XX ?? ?? ?? ?? XX
// Change both 'XX' to one of your choise:
//41 = Warding (Def +4)
//42 = Arcane (Mana +20)
//44 = Lucky (Crit Chance +4%)
//48 = Menacing (Damage +4%)
//4C = Quick (Mov Speed +4%)
//50 = Violent (Melee Speed +4%)

mem:
flag:
dd 0
// Check for Invalid Prefix
invalid_prefix_mem:
mov [ebp-24],edx
mov [flag],0
fild dword ptr [ebp-24]
jmp invalid_prefix_ret

// Melee Weaps
legendary_mem:
cmp [flag],0
jne demonic_ret // Fix Prefix
mov eax,27
mov [flag],1
@@:
cmp eax,27
jne legendary_ret
mov ebx,51
jmp legendary_ret

// Fix (eg. Drax and some Others Melee)
godly_mem:
cmp [flag],0
jne godly_ret
add [flag],1
mov eax,B
jmp godly_ret

// Ranged Weaps
unreal_mem:
cmp [flag],0
jne godly_ret // Fix Prefix
mov eax,23
mov [flag],1
@@:
cmp eax,23
jne unreal_ret
mov ebx,52
jmp unreal_ret

// Magical Weaps
mythical_mem:
cmp [flag],0
jne demonic_ret // Fix Prefix
mov eax,23
mov [flag],1
@@:
cmp eax,23
jne mythical_ret
mov ebx,53
jmp mythical_ret

[DISABLE]
force_prefix:
db 75
invalid_prefix:
db 89 55 DC DB 45 DC
legendary:
db 0F 85 33 10 00 00 BB 51 00 00 00
godly:
db 83 F8 0B 75 05
unreal:
db 0F 85 7C 08 00 00 BB 52 00 00 00
mythical:
db 0F 85 10 03 00 00 BB 53 00 00 00
accessories:
db 6A 51 8B 4D E0 BA 3E
dealloc(mem)
</AssemblerScript>
